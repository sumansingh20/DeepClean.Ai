# Frontend Environment Variables (Vercel)

# API Configuration
NEXT_PUBLIC_API_URL=https://your-backend-url.com/api/v1
NEXT_PUBLIC_WS_URL=wss://your-backend-url.com/ws

# OAuth - Google
NEXT_PUBLIC_GOOGLE_CLIENT_ID=your-google-client-id
NEXT_PUBLIC_GOOGLE_REDIRECT_URI=https://your-domain.vercel.app/auth/callback/google

# OAuth - Facebook
NEXT_PUBLIC_FACEBOOK_APP_ID=your-facebook-app-id
NEXT_PUBLIC_FACEBOOK_REDIRECT_URI=https://your-domain.vercel.app/auth/callback/facebook

# OAuth - Instagram
NEXT_PUBLIC_INSTAGRAM_CLIENT_ID=your-instagram-client-id
NEXT_PUBLIC_INSTAGRAM_REDIRECT_URI=https://your-domain.vercel.app/auth/callback/instagram

# App Configuration
NEXT_PUBLIC_APP_NAME=Deepfake Detection Platform
NEXT_PUBLIC_APP_URL=https://your-domain.vercel.app
